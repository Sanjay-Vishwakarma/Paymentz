        function hideshowpass(spanid,inputid)
        {
            var x = document.getElementById(inputid);
            if (x.type === "password")
            {
                $("#"+spanid).removeClass('fa-eye-slash').addClass('fa-eye')
                x.type = "text";
            }
            else
            {
                $("#"+spanid).removeClass('fa-eye').addClass('fa-eye-slash')
                x.type = "password";
            }
        }


